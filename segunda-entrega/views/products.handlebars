<div class="container" >
<div class="row">
	<div class="col-md-12">
		<h2>Carga de productos</h2>
		<form id="carga">
			<div class="col-md-4">
				<label for="title">Title</label>
				<input type="text" name="title" placeholder="Title" class="form-control" id="title" >	
			</div>
			<div class="col-md-4">
				<label for="description">Description</label>
				<input type="text" name="description" placeholder="Description" class="form-control" id="description" >
			</div>
			<div class="col-md-4">
				<label for="code">Code</label>
				<input type="text" name="code" placeholder="Code" class="form-control" id="code" >
			</div>
			<div class="col-md-4">
				<label for="price">Price</label>
				<input type="number" name="price" placeholder="Price" class="form-control" id="price" >
			</div>
			<div class="col-md-4">
				<label for="stock">Stock</label>
				<input type="number" name="stock" placeholder="Stock" class="form-control" id="stock" >
			</div>
			<div class="col-md-4">
				<label for="status">Status</label>
				<input type="text" name="status" placeholder="Status" class="form-control" id="status" >
			</div>
			<div class="col-md-4">
				<label for="category">Category</label>
				<input type="text" name="category" placeholder="Category" class="form-control" id="category" >
			</div>
			<div class="col-md-4">
				<label for="thumbnail">Thumbnail</label>
				<input type="text" name="thumbnail" placeholder="Thumbnail" class="form-control" id="thumbnail" >
			</div>
			<div class="col-md-4">
				<button type="submit" class="btn btn-primary">Enviar</button>
			</div>

		
 
    <div class="col-md-12">
        <h2>Productos</h2>
		<div ></div>
        <table class='table' >
			<thead>
				<tr>
					<th scope='col'>ID</th>
					<th scope='col'>Title</th>
					<th scope='col'>Description</th>
					<th scope='col'>Code</th>
					<th scope='col'>Price</th>
					<th scope='col'>Stock</th>
					<th scope='col'>Status</th>
					<th scope='col'>Category</th>
					<th scope='col'>Thumbnail</th>
				</tr>
			</thead>
			<tbody id="realTimeProducts"></tbody> 
		</table>
	</div>


    </div> 
</div>
</div>


<script> var page = 'products' </script> 


